mixin doubleValuesBar(options) 
  -const {val1, val2} = options || {};
  .double-values-bar(data-val1=val1, data-val2=val2, class!= attributes.class)
    .col
      .first-bar(style={width: 0}) 
        .results 
          .first-result 0
          span.slash /
          .second-result 0
      .second-bar(style={width: 0})
